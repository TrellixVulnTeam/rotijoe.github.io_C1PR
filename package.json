{
  "name": "poketypes",
  "version": "1.0.0",
  "description": "pokemon go battle app",
  "main": "script.js",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.1.0",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "scripts": {
    "watch:sass": "node-sass main.scss style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass main.scss style.comp.css",
    "concat:css": "concat -o style.concat.css icon-font.css style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' style.concat.css -o style.prefix.css",
    "compress:css": "node-sass style.prefix.css style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "joe",
  "license": "ISC"
}
